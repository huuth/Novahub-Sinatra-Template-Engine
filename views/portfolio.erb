	        <% img_src.each_with_index do |src, index|%>
	        	<% type = types[index]%>
	        	<% str_type = type.is_a?(Array) ? type * " " : type%>
				<!-- Portfolio Item -->
                <div class="col-lg-3 col-md-3 col-sm-4 col-xs-6 portfolio-item <%= str_type %>" >
                    <div class="portfolio-box">
                        <div class="mask-hover col-md-3 col-sm-6 col-xs-12">
													<div class="link-mask">
														<img src=<%= src%> class="img-responsive">
														<div class="link-preview">
